<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="line">
            <stroke android:width="@dimen/slider_track_height" android:color="@color/kuromi_purple" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="line">
                <stroke android:width="@dimen/slider_track_height" android:color="@color/kuromi_pink" />
            </shape>
        </clip>
    </item>
</layer-list>
